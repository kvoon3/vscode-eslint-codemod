{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "schedule": ["every weekend"],
  "packageRules": [
    {
      "matchPackageNames": ["eslint-plugin-command"],
      "matchPackagePatterns": ["eslint-plugin-command"],
      "automerge": true,
      "schedule": ["every weekend"]
    },
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    }
  ]
}
